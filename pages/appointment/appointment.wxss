/* pages/appointment/appointment.wxss */

/* @import "../../weui.wxss"; */
@import "../../wxss/icon.wxss";

.appointment {
  padding: 0 30rpx;
}

.appointment_top {
  display: flex;
  justify-content: space-between;
}

.appointment_top> .iconfont {
  font-size: 98rpx;
}

.left {
  width:60%;
  height: 100rpx;
  line-height: 100rpx;
}
.left>.left-text{
  font-size: 34rpx;
}

.right {
  width: 45%;
  text-align: right;
  height: 100rpx;
  line-height: 100rpx;
  display: inline-block;
}

.right>text:nth-child(1) {
  margin-right: 40rpx;
}

.main {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  grid-gap: 50rpx;
  padding: 0 20rpx;
  margin: 20rpx auto;
}

.main > .item {
  text-align: center;
  height: 160rpx;
  font-size: 24rpx;
  margin: 0 auto;
  padding: 20rpx 20rpx 0;
}

.meeting-in-pro {
  width: 100%;
  border-radius: 10px;
  padding: 10rpx 0 40rpx 0;
  background: linear-gradient(60deg, #24a8fd, #fb6aed);
}

.meeting-top {
  color: #fff;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 30rpx;
}

.metting-left {
  float: left;
  width: 50%;
  font-size: 34rpx;
}

.metting-right {
  float: right;
  width: 30%;
  margin-top: 10rpx;
}

.meeting-top-room {
  width: 100%;
}

.meeting-top-del {
  display: flex;
  color: #fff;
  padding: 0 30rpx;
}

.metting-num-big {
  font-size: 42rpx;
  margin-left: -25rpx;
}

.meeting-fel-small {
  font-size: 24rpx;
  color: #dbe2fd;
  line-height: 80rpx;
}

.meeting-top-text {
  /* flex: 1; */
  text-align: center;
  /* float: left; */
}

.meeting-huiYi {
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  padding: 0 45rpx;
  font-size: 30rpx;
}

.meeting-in-room {
  width: 100%;
  text-align: center;
  color: #fff;
  margin-top: 20rpx;
}

.meeting-in-room-btn {
  font-size: 46rpx;
  padding: 5rpx;
  margin: 0 45rpx;
  border-radius: 40rpx;
  font-weight: 300;
  background-image: -webkit-linear-gradient(60deg, #f18ccc, #74defb);
  background-image: -moz-linear-gradient(60deg, #f18ccc, #74defb);
  background-image: linear-gradient(60deg, #f18ccc, #74defb);
  box-sizing: border-box;
}

.meeting-in-room-child {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  background: linear-gradient(60deg, #24a8fd, #d874ee);
}

.metting-right>.btn_click {
  width: 48rpx;
  height: 48rpx;
  border: 0;
  padding: 0;
  color: #fff;
  float: right;
  margin-left: 20rpx;
  background: #ffffff24;
  border-radius: 48rpx;
  line-height: 48rpx;
  text-align: center;
  font-size: 32rpx;
}

.liner-circle {
  width: 120rpx;
  height: 120rpx;
  /* box-sizing: border-box; *//* border: 1px solid #333; *//* border-image: linear-gradient(#F80, #2ED) 20 20; *//* background-image: -webkit-linear-gradient(top, #e977bc 0%, #e977bc 30%, #8c86cb 60%, #98dcf6 90%);
  background-image: -moz-linear-gradient(top, #e977bc 0%, #e977bc 30%, #8c86cb 60%, #98dcf6 90%);
  background-image: linear-gradient(top, #e977bc 0%, #e977bc 30%, #8c86cb 60%, #98dcf6 90%); */
  border-radius: 50%;
  margin-bottom: 10rpx;
  text-align: center;
  line-height: 120rpx;
  /* padding: 5rpx; */
}

.circleBorCorBl {
  border: 2px solid #10aeff;
}

.circleBorCorRe {
  border: 2px solid #f76260;
}

.circleBorCorVi {
  border: 2px solid #8a6cec;
}

.box {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background: #fff;
}

.box> .iconfont {
  font-size: 50rpx;
}

.left .icon-tianjiashebei {
  display: inline-block;
  margin-left: 20rpx;
  font-size: 32rpx;
}

.icon-icon-qiehuan {
  display: inline-block;
  transform: rotate(90deg);
  margin-left: 20rpx;
  font-size: 35rpx;
}

.icon-search {
  font-size: 32rpx;
  color: #4b4b4b;
}

.icon-saoma {
  font-size: 32rpx;
  color: #4b4b4b;
}

.icon-kuaisu-copy {
  color: #0faeff;
}

.icon-jiahao {
  color: #f76260;
}

.icon-yuyue {
  color: #7348fe;
}

/* tab样式start */

.tabNav {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  height: 100rpx;
  border-radius: 20rpx 20rpx 0 0;
  /* background: linear-gradient(to right, #e977bc 0%, #27a9ff 100%); */
  background: #10aeff;
  margin-top: 5%;
  padding: 0 20rpx;
}

.tabNav>view {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
}

.tabNav view>.text_l {
  float: left;
  padding: 0 15rpx;
}

.tabNav view>.text_r {
  float: right;
  padding: 0 15rpx;
}

.tabNav .cur text {
  font-size: 30rpx;
}

.pend-meeting{
  font-size: 24rpx;
  color: #333;
  padding: 0rpx 30rpx;
  margin-bottom: 75rpx;
}
.showDevice{
  display:block;
}

.show {
  display: grid;
}

.hide {
  display: none;
}

.info-no-content {
  padding-top: 106rpx;
  text-align: center;
}

.info-no-content image {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  display: block;
}

.info-no-content text {
  position: relative;
  top: 30rpx;
  font-size: 28rpx;
  color: #68686F;
  padding-top: 20rpx;
}

.info-content {
  border-bottom: 1px solid #ddd;
  height: 200rpx;
  padding: 0;
}

.info-top {
  display: flex;
  justify-content: space-between;
}

.info-left {
  width: 70%;
}

.info-right {
  width: 30%;
}

.font-time {
  width: auto;
  border: 1rpx solid #fa6aec;
  padding: 0 20rpx;
  border-radius: 30rpx;
  color: #fa6aec;
  font-size: 22rpx;
}

.font30 {
  font-size: 36rpx;
  color: #000;
  padding: 24rpx 0 8rpx 0;
  position: relative;
  height: 38rpx;
}

.fontroom {
  float: left;
  margin-right: 20rpx;
}

.font-timeS {
  float: left;
  margin-top: 10rpx;
}

.font20 {
  font-size: 28rpx;
  color: #8d8d92;
  padding: 14rpx 0 0 0;
}

.action {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  border: 1px solid #27a9ff;
  text-align: center;
  float: right;
  color: #27a9ff;
  margin: 20rpx -10rpx 20rpx 0rpx;
  font-size: 29rpx;
}

.meet-time {
  color: #333;
}

.noaction {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  border: 1px solid #c8c8cd;
  text-align: center;
  float: right;
  color: #797979;
  margin: 20rpx -10rpx 20rpx 0rpx;
  background: #c8c8cd;
  font-size: 30rpx;
}

.time {
  color: #27a9ff;
  float: right;
}

.btn>.iconfont {
  font-size: 24rpx;
}

.info-button>.btn {
  display: inline-block;
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #ddd;
  border-radius: 25rpx;
  text-align: center;
  margin-right: 20rpx;
  margin-top: 20rpx;
}

.fuzhi-fenX {
  width: 176rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  float: right;
  margin-left: 60rpx;
  padding-top: 28rpx;
}

.fuzhi-fenX>.btn_click {
  width: 60rpx;
  height: 60rpx;
  border: none;
  padding: 0;
  color: #8d8d92;
  float: right;
  margin-left: 20rpx;
  line-height: 60rpx;
  font-size: 34rpx;
}

.aa{
  background: rgba(0,0,0, 0.5);
}
